<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.web.controller.entity">
	<class name="Education" table="tsdr_education">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="category" />
		<property name="type" />
		<property name="cateclass" />
		
		<property name="contents" />
		
		<property name="trainerName" />
		<property name="department" />
		
		<property name="schedule" />
		<property name="duration" />
		
		<property name="trainee" />
		<property name="traineeObject" />
		<property name="traineeTotal" />
		<property name="coverage" />
		
		<property name="trainingTime" />
		<property name="trainingDiffculty" />
		<property name="trainerEvaluation" />
		
		<property name="materials" />
		<property name="filename" />
		<property name="ip" />	
		<property name="uploadTime" />		
	</class>
</hibernate-mapping>